{
   "installCommand": "npm install --lagacy-peer-deps",
   "version": 2,
   "builds":[
    {
        "src": "server.ts",
        "use": "@vercel/node"
    },
    {
        "src": "src/**/*",
        "use": "@vercel/static"
    }
   ],
   "routes":[
    {"src": "/(./*)",
     "dest": "src/server.ts",
     "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
     "headers":{
        "Access-Control-Allow-Origin": "*"
     } 
    }
   ]
}